{
  "name": "enabler",
  "private": true,
  "version": "4.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "start": "serve public -l 8080 --cors",
    "lint": "eslint src --ext .ts,.js",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --detectOpenHandles --coverage --collectCoverageFrom='src/**/*.{ts,jsx,tsx,js}' --coverageDirectory='./coverage'",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "npm run build && serve public -l 3000 --cors"
  },
  "dependencies": {
    "@sinclair/typebox": "0.34.41",
    "serve": "14.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.1",
    "eslint": "^9.39.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-environment-node": "30.2.0",
    "sass": "1.94.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "7.2.4",
    "vite-plugin-css-injected-by-js": "3.5.2"
  },
  "overrides": {
    "minimatch": "10.2.1",
    "glob": "11.1.0"
  }
}
